<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="biometricgatesecurity.ResidentVisitorLandingController">
    <stylesheets>
        <URL value="@ResidentLanding.css" />
    </stylesheets>
   <children>
      <HBox layoutX="75.0" layoutY="167.0" prefHeight="100.0" prefWidth="470.0" spacing="20.0">
         <children>
            <Button mnemonicParsing="false" onAction="#registorVisitor" prefHeight="46.0" prefWidth="124.0" text="ADD" textAlignment="RIGHT" wrapText="true">
               <font>
                  <Font name="Bell MT" size="18.0" />
               </font>
               <graphic>
                  <ImageView fitHeight="75.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/add.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="edit" mnemonicParsing="false" onAction="#editVisitor" prefHeight="46.0" prefWidth="124.0" text="EDIT" textAlignment="RIGHT" wrapText="true">
               <font>
                  <Font name="Bell MT" size="18.0" />
               </font>
               <graphic>
                  <ImageView fitHeight="75.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/edit.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="delete" mnemonicParsing="false" onAction="#deleteVisitor" prefHeight="62.0" prefWidth="159.0" text="DELETE" textAlignment="RIGHT" wrapText="true">
               <font>
                  <Font name="Bell MT" size="18.0" />
               </font>
               <graphic>
                  <ImageView fitHeight="75.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/delete.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </children>
         <padding>
            <Insets left="10.0" top="20.0" />
         </padding>
      </HBox>
      <Button layoutX="24.0" layoutY="21.0" mnemonicParsing="false" onAction="#back" prefHeight="33.0" prefWidth="113.0" text="BACK">
         <font>
            <Font name="Bell MT" size="18.0" />
         </font>
         <graphic>
            <ImageView fitHeight="29.0" fitWidth="34.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/back.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Label layoutX="222.0" layoutY="112.0" prefHeight="40.0" prefWidth="156.0" text="VISITORS">
         <font>
            <Font name="Algerian" size="36.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
