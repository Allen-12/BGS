<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ComboBox fx:id="selectHouse" layoutX="199.0" layoutY="185.0" prefHeight="30.0" prefWidth="202.0" promptText="Please select a house" />
      <Label layoutX="162.0" layoutY="95.0" text="SELECT HOUSE">
         <font>
            <Font name="Bell MT Bold" size="36.0" />
         </font>
      </Label>
      <Button fx:id="registerResidents" layoutX="194.0" layoutY="273.0" mnemonicParsing="false" onAction="#goToRegisterOccupants" text="REGISTER OCCUPANT">
         <font>
            <Font name="Times New Roman" size="18.0" />
         </font>
      </Button>
      <Button fx:id="registerVehicle" layoutX="194.0" layoutY="311.0" mnemonicParsing="false" onAction="#goToRegisterVehicle" prefHeight="32.0" prefWidth="212.0" text="REGISTER VEHICLE">
         <font>
            <Font name="Times New Roman" size="18.0" />
         </font>
      </Button>
      <Button layoutX="14.0" layoutY="24.0" mnemonicParsing="false" onAction="#back" prefHeight="33.0" prefWidth="113.0" text="BACK">
         <font>
            <Font name="Bell MT" size="18.0" />
         </font>
         <graphic>
            <ImageView fitHeight="29.0" fitWidth="34.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/back.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
</AnchorPane>
